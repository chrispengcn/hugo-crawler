{
  "project_root": "my_blog_project3",
  "fields": {
    "title": {
      "xpath": "//h1",
      "attribute": "text",
      "front_matter": true
    },
    "featureImage": {
      "xpath": "/html/body/div[2]/div/div[1]/div[3]/img",
      "attribute": "src",
      "front_matter": true
    },
    "content": {
      "xpath": "/html/body/div[2]/div/div[1]/div[4]/div",
      "attribute": "html",
      "front_matter": false
    },
    "date": {
      "xpath": "//time[@class='article-date']",
      "attribute": "datetime",
      "front_matter": true
    },  
    "category": {
      "xpath": "//*[@id='page-head']/div/div[1]/nav/ol/li[last()]",
      "attribute": "text",
      "front_matter": true
    }
  }
}
